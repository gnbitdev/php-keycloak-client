{
    "name": "gnbit/php-keycloak-client",
    "license": "MIT",
    "keywords": [
        "sdk",
        "keycloak",
        "jwt",
        "authentication"
    ],
    "authors": [
        {
            "name": "Joan Perez Alvarado",
            "email": "41269373+joanjpx@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2 || ^7.3 || ^7.4 || ^8",
        "guzzlehttp/guzzle": "^7.0",
        "league/oauth2-client": "^2.6",
        "firebase/php-jwt": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "KeycloakApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeycloakApiCLientTest\\": "test/unit",
            "KeycloakApiCLientIntegrationTest\\": "test/integration"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
